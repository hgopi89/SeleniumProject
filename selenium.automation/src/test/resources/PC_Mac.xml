<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Test on Mac" verbose="1" parallel="false"
	thread-count="3" time-out="600000">
	<parameter name="platform" value="Mac" />

	<test name="Website- Firefox" preserve-order="true" parallel="false">
		<parameter name="browser" value="firefox" />
		<classes>
		 	<class name="selenium.automation.testcase.AppTest"></class> 
		</classes>
	</test>

	<test name="Website- Chrome" preserve-order="true" parallel="false">
		<parameter name="browser" value="chrome" />
		<classes>
			  <class name="selenium.automation.testcase.AppTest"></class>
			
		</classes>
	</test>
</suite>